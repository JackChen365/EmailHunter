{
    "name":"Email hunter",
    "description":"Base Level Extension",
    "version":"1.0",
    "manifest_version":2,
    "browser_action":{
        "default_popup":"email_hunter.html",
        "default_icon":"image/find_in_page.png"
    },
    "commands":{
        "_execute_browser_action":{
            "suggested_key":{
                "default":"Ctrl+Shift+F",
                "mac":"MacCtrl+Shift+F"
            },
            "description":"Opens email_hunter.html"
        }
    },
    "background": {
        "scripts": ["js/background.js"]
    },
    "permissions":[
        "tabs",
        "downloads",
        "*://*/"
    ]
    
}